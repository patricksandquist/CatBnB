<h1>Sign in</h1>

<%= render 'shared/errors' %>

<form action="<%=session_url%>" method='POST'>
  <label for='username'>Username</label>
  <input
    type='text'
    name='user[username]'
    id='username'
    value="<% @user.username %>">
  <br>

  <label for='password'>Password</label>
  <input
    type='password'
    name='user[password]'
    id='password'>
  <br>
  <br>
  <input type='submit' value='Sign in'>
</form>
